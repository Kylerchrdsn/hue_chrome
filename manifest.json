{
  "manifest_version": 2,

  "name": "Hue for Chrome",
  "description": "A chrome extension for controlling hue lights.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
